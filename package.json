{
  "name": "kamasi",
  "version": "0.1.0",
  "description": "Music theory library",
  "author": "Vegard Egeland",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.js",
  "imports": {
    "#src/*": "./src/*",
    "#data/*": "./data/*"
  },
  "exports": {
    ".": "./src/index.js",
    "./interval": "./src/interval.js",
    "./note": "./src/note.js",
    "./notelist": "./src/notelist.js",
    "./search": "./src/search.js",
    "./data/intervals": "./data/intervals.js",
    "./data/scales": "./data/scales.js",
    "./data/chords": "./data/chords.js"
  },
  "scripts": {
