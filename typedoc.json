  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "exclude": ["data/**", "**/*.test.ts", "test/**/*"],
  "excludePrivate": true,
  "excludeInternal": true,
  "readme": "README.md",
  "navigationLinks": {
    "GitHub": "https://github.com/vegardege/kamasi"
